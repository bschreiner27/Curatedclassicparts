{
  "name": "rrp-api",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc -p .",
    "start": "node dist/index.js",
    "db:generate": "drizzle-kit generate --config=./db/drizzle.config.ts",
    "db:migrate": "drizzle-kit migrate --config=./db/drizzle.config.ts",
    "db:push": "drizzle-kit push --config=./db/drizzle.config.ts",
    "db:seed": "tsx ./db/seed/seed.ts",
    "job:garage": "tsx ./scripts/enqueueGarageMatches.ts"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "drizzle-orm": "^0.32.0",
    "express": "^4.19.2",
    "pg": "^8.12.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "drizzle-kit": "^0.24.0",
    "tsx": "^4.16.2",
    "typescript": "^5.5.4"
  }
}
